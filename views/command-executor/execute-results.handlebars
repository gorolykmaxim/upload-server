{{> navigation isCommandExecutor=true }}

<div class="container">
    <h3 class="mt-3">{{name}}</h3>
    <p>Command: '{{command.command}}'</p>
    <p>Timeout: {{command.timeout}}</p>
    <h3 class="mt-1">STDOUT:</h3>
    {{#each stdout}}
        <p>{{this}}</p>
    {{/each}}
    <h3 class="mt-1">STDERR:</h3>
    {{#each stderr}}
        <p>{{this}}</p>
    {{/each}}
</div>
